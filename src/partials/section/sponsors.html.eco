<ul class="organizations">
    <% for sponsor in @sponsors: %>
    <li itemscope itemtype="http://schema.org/Organization">
        <a href="<%= sponsor.url %>" itemprop="url">
            <img src="<%= sponsor.logo %>" alt="<%= sponsor.name %>" itemprop="image">
        </a>
    </li>
    <% end %>
</ul>